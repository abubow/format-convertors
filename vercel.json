{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "cleanUrls": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      }
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/(.*\\.(js|json|css|ico|png|jpg|jpeg|svg|webp|woff|woff2|ttf|otf|mp4)$)",
      "dest": "/$1",
      "headers": { 
        "cache-control": "public,max-age=31536000,immutable" 
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index"
    }
  ],
  "env": {
    "VERCEL": "1"
  }
} 